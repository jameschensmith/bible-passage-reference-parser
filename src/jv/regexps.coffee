bcv_parser::regexps.third = "(?:III|3)\\.?#{bcv_parser::regexps.space}*"
bcv_parser::regexps.range_and = "(?:[&\u2013\u2014-]|&|-)"
bcv_parser::regexps.range_only = "(?:[\u2013\u2014-]|-)"
